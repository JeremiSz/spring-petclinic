pipeline{
    agent{
        docker{
            image 'maven:3-openjdk-18'
        }
    }
    tools{
        codeQL 'codeql'
    }
    stages{
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
                codeQL 'init'
                codeQL 'autobuild'
                codeQL 'analyze'
            }
        }
    }
}