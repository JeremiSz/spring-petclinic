pipeline{
    agent{
        docker{
            image 'maven:3-openjdk-18'
        }
    }
    tools{
        codeql 'codeql'
    }
    stages{
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
                codeql 'autobuild'
                codeql 'analyze'
            }
        }
    }
}